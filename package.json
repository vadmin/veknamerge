{
  "name": "veknamerge",
  "displayName": "veknaMerge",
  "description": "Converts ORA insert into Merge statement",
  "version": "0.0.4",
  "publisher": "Igor Hrustic",
  "repository": {
    "type": "git"
  },
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "enabledApiProposals": ["telemetry"],
  "activationEvents": [
    "onCommand:extension.convertInsertToMerge"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.convertInsertToMerge",
        "title": "Vekna: ORA Insert to Merge"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.convertInsertToMerge",
          "when": "editorHasSelection",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode development",
    "watch": "webpack --watch",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/prettier": "^2.7.3",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "sql-formatter": "^15.4.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "uuid": "^10.0.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "veknamerge": "file:"
  }
}
